<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="logicgates.mytextview.com.logicgates">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".andActivity"
            android:label="@string/title_activity_and"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".orActivity"
            android:label="@string/title_activity_or"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".notActivity"
            android:label="@string/title_activity_not"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".allInOneActivity"
            android:label="@string/title_activity_all_in_one"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".XoRActivity"></activity>
    </application>

</manifest>
